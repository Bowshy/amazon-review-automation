{
	"name": "amazon-review-automation",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:setup": "npm run db:generate && npm run db:migrate",
		"db:reset": "npm run db:push",
		"automation:run-daily": "tsx src/scripts/run-daily-automation.ts",
		"automation:inventory-ledger-sync": "tsx src/scripts/inventory-ledger-daily-sync.ts",
		"report:download": "tsx scripts/download-report.ts",
		"tsv:to-csv": "tsx scripts/tsv-to-csv.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^24.3.0",
		"@types/pg": "^8.15.6",
		"bits-ui": "^2.14.2",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.31.6",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.20.4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@elastic/ecs-winston-format": "^1.5.3",
		"amazon-sp-api": "^1.1.6",
		"bcryptjs": "^2.4.3",
		"csv-parse": "^5.6.0",
		"csv-stringify": "^6.6.0",
		"date-fns": "^4.1.0",
		"dotenv": "^17.2.1",
		"drizzle-orm": "^0.44.7",
		"lucide-svelte": "^0.552.0",
		"pg": "^8.16.3",
		"postgres": "^3.4.7",
		"undici": "^6.22.0",
		"winston": "^3.17.0"
	}
